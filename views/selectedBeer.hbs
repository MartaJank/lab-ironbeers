<div class="selected-beer-div">
    {{#each beersFromApi}}
    <div class="other-selec-div">
        {{> beerpartial this}}
    </div>
    <div>
        <p>First brewed: {{first_brewed}}</p>
        <p>abv: {{abv}}</p>
        <p>ibu: {{ibu}}</p>
        <p>target fg: {{target_fg}}</p>
        <p>target og: {{target_og}}</p>
        <p>ebc: {{ebc}}</p>
        <p>srm: {{srm}}</p>
        <p>ph: {{ph}}</p>
        <p>Attenuation level: {{attenuation_level}}</p>
        <p>Volume: {{volume.value}} {{volume.unit}}</p>
        <p>Boil volume: {{boil_volume.value}} {{boil_volume.unit}}</p>
        <p>Food pairing: {{food_pairing}}</p>
        <p>Brewers tips: {{brewers_tips}}</p>
        <p>Contributed by: {{contributed_by}}</p>
    </div>
    {{/each}}
</div>